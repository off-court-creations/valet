{
  "name": "DateSelector",
  "category": "fields",
  "slug": "components/fields/dateselector",
  "summary": "DateSelector component",
  "status": "experimental",
  "aliases": [
    "date",
    "datepicker",
    "daterange"
  ],
  "props": [
    {
      "name": "value",
      "type": "string",
      "required": false,
      "default": "—",
      "description": "Controlled ISO date value (YYYY-MM-DD)"
    },
    {
      "name": "defaultValue",
      "type": "string",
      "required": false,
      "default": "—",
      "description": "Initial uncontrolled value"
    },
    {
      "name": "onChange",
      "type": "(value: string) => void",
      "required": false,
      "default": "—",
      "description": "Fires when selection changes"
    },
    {
      "name": "name",
      "type": "string",
      "required": false,
      "default": "—",
      "description": "FormControl field name"
    },
    {
      "name": "minDate",
      "type": "string",
      "required": false,
      "default": "'120y ago'",
      "description": "Earliest selectable date"
    },
    {
      "name": "maxDate",
      "type": "string",
      "required": false,
      "default": "'120y ahead'",
      "description": "Latest selectable date"
    },
    {
      "name": "range",
      "type": "boolean",
      "required": false,
      "default": "false",
      "description": "Enable dual start/end selection"
    },
    {
      "name": "endValue",
      "type": "string",
      "required": false,
      "default": "—",
      "description": "Controlled end date when range is true"
    },
    {
      "name": "defaultEndValue",
      "type": "string",
      "required": false,
      "default": "—",
      "description": "Uncontrolled end date default"
    },
    {
      "name": "onRangeChange",
      "type": "(start: string, end: string) => void",
      "required": false,
      "default": "—",
      "description": "Fires when range selection changes"
    },
    {
      "name": "sx",
      "type": "Sx",
      "required": false
    },
    {
      "name": "compactMode",
      "type": "'auto' | 'on' | 'off'",
      "required": false,
      "default": "'auto'",
      "enumValues": [
        "auto",
        "on",
        "off"
      ],
      "description": "Control compact behavior: auto-detect, force on, or disable entirely"
    },
    {
      "name": "compactThresholdIn",
      "type": "number",
      "required": false,
      "default": "340",
      "description": "Width below which compact turns on (only when compactMode=auto)"
    },
    {
      "name": "compactThresholdOut",
      "type": "number",
      "required": false,
      "default": "380",
      "description": "Width above which compact turns off (only when compactMode=auto)"
    },
    {
      "name": "preset",
      "type": "string | string[]",
      "required": false,
      "default": "—",
      "description": "Apply style presets"
    }
  ],
  "cssVars": [
    "--valet-date-radius",
    "--valet-date-cell-radius",
    "--valet-text-color"
  ],
  "cssPresets": [],
  "events": [
    {
      "name": "onChange",
      "payloadType": "string"
    },
    {
      "name": "onRangeChange",
      "payloadType": "string"
    }
  ],
  "actions": [],
  "slots": [],
  "bestPractices": [],
  "examples": [],
  "docsUrl": "/components/field/DateSelectorDemo",
  "sourceFiles": [
    "src/components/fields/DateSelector.tsx",
    "docs/src/pages/components/field/DateSelectorDemo.tsx"
  ],
  "version": "0.31.0",
  "schemaVersion": "1.5"
}