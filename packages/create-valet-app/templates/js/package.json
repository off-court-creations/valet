{
  "name": "valet-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite --host",
    "build": "npm run -s clean && vite build",
    "lint": "eslint \"src/**/*.{js,jsx}\" && echo \"No ESLint issues found.\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix && echo \"No ESLint issues found.\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.{js,jsx,json,md,css}\" --check",
    "format:fix": "prettier --ignore-path .gitignore \"src/**/*.{js,jsx,json,md,css}\" --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "@archway/valet": "^0.31.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.37.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "vite": ">=6.4.1"
  }
}
