{
  "name": "@archway/valet",
  "version": "0.5.2",
  "description": "valet is a CSS-in-JS engine, a UI component kit, and an accessibility layer that treats all humans and their AI proxies as first-class users.",
  "keywords": [
    "react",
    "ai",
    "ui",
    "fiber",
    "react-component",
    "archway",
    "occ"
  ],
  "author": "0xbenc",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@emotion/hash": "^0.9.2",
    "@iconify/react": "^6.0.0",
    "zustand": "^4.5.7"
  },
  "peerDependencies": {
    "react": ">=18.0.0 || ^19.0.0",
    "react-dom": ">=18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "babel-loader": "^10.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/off-court-creations/valet.git"
  },
  "bugs": {
    "url": "https://github.com/off-court-creations/valet/issues"
  },
  "homepage": "https://github.com/off-court-creations/valet#readme"
}
