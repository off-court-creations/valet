{
  "name": "@archway/valet",
  "version": "0.33.5",
  "description": "valet is a CSS-in-JS engine, a UI component kit, and an accessibility layer that treats all humans and their AI proxies as first-class users.",
  "keywords": [
    "react",
    "ai",
    "ui",
    "fiber",
    "react-component",
    "archway",
    "occ"
  ],
  "author": "0xbenc",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./styles.css"
  },
  "files": [
    "dist",
    "styles.css",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs --dts --tsconfig tsconfig.build.json && cp -f src/styles.css styles.css",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "dx": "npm install && npm --prefix ./docs install && npm --prefix ./packages/create-valet-app install && npm run mcp:server:install",
    "dx:link": "npm run build && npm link && cd docs && npm link @archway/valet && cd ../packages/valet-mcp && npm link && cd ../create-valet-app && npm link",
    "lint": "eslint \"{src,docs}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,docs}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --ignore-path .gitignore \"{src,docs}/**/*.{ts,tsx,js,jsx,json,md,css}\" --check",
    "format:fix": "prettier --ignore-path .gitignore \"{src,docs}/**/*.{ts,tsx,js,jsx,json,md,css}\" --write",
    "mcp:build": "node scripts/mcp/merge.mjs",
    "mcp:watch": "node --watch scripts/mcp/merge.mjs",
    "mcp:schema:check": "node scripts/mcp/validate.mjs",
    "mcp:server:install": "cd packages/valet-mcp && npm install",
    "mcp:server:build": "cd packages/valet-mcp && npm run build",
    "mcp:server:start": "cd packages/valet-mcp && npm run start",
    "mcp:server:publish": "npm run mcp:build && cd packages/valet-mcp && npm publish --access public",
    "mcp:server:publish:patch": "npm run mcp:build && cd packages/valet-mcp && npm version patch --no-git-tag-version && npm publish --access public",
    "mcp:server:publish:minor": "npm run mcp:build && cd packages/valet-mcp && npm version minor --no-git-tag-version && npm publish --access public",
    "mcp:server:publish:major": "npm run mcp:build && cd packages/valet-mcp && npm version major --no-git-tag-version && npm publish --access public",
    "cva:install": "npm --prefix packages/create-valet-app install",
    "cva:validate": "npm --prefix packages/create-valet-app run validate"
  },
  "dependencies": {
    "@iconify/react": "^6.0.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.1.1",
    "marked-highlight": "^2.2.2",
    "react-dropzone": "^14.2.3",
    "zustand": "^4.5.7"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/parser": "^7.27.2",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.25.9",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.25.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-morph": "^23.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/off-court-creations/valet.git"
  },
  "bugs": {
    "url": "https://github.com/off-court-creations/valet/issues"
  },
  "homepage": "https://github.com/off-court-creations/valet#readme"
}
